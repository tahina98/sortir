<!DOCTYPE html>
<html   lang="fr" data-theme="winter">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Sortir.com{% endblock %}</title>
        <link rel="icon" type="image/x-icon" href="{{ asset('build/logo.png') }}">

            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_script_tags('app') }}

    </head>
    <body >
    <div class="contenuPage">
{#nav-bar#}
<nav class="navBarSortir bg-white dark:bg-gray-900 fixed w-full z-20 top-0 left-0 border-b border-gray-200 dark:border-gray-600">
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
        <a href="{{ path('main_home') }}" class="flex items-center">
            <img class="imgNav" src="{{ asset('build/logo.png') }}" alt="logo">
            <span class="sortirTitre self-center text-2xl font-semibold whitespace-nowrap dark:text-white">Sortir.com</span>
        </a>
        <div class=" nomId flex md:order-2">
            {% if app.user %}
                <p>{{ app.user.userIdentifier }} <span class="connecté">connecté</span></p>
            {% endif %}
        </div>
        <div class="items-center justify-between hidden w-full md:flex md:w-auto md:order-1" id="navbar-sticky">
            <ul class="ulNav flex flex-col p-4 md:p-0 mt-4 font-medium border border-gray-100 rounded-lg bg-gray-50 md:flex-row md:space-x-8 md:mt-0 md:border-0 dark:border-gray-700">

                <li>
                    <a href="{{ path('sortie_liste') }}" class="liste block py-2 pl-3 pr-4 text-white  rounded md:bg-transparent  md:p-0 md:dark:text-white-500" aria-current="page">La liste</a>
                </li>
                {% if app.user %}
                    <li>
                        <a href="{{ path('main_profil') }}"
                           class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">Mon
                            Profil</a>
                    </li>
                    <li>
                        <a href="{{ path('app_logout') }}"
                           class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">Déconnexion</a>
                    </li>
                    <li>
                        <a href="{{ path('sortie_liste_a_publier') }}"
                           class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">A
                            publier</a>
                    </li>
                {% endif %}
                {% if not app.user %}
                    <li>
                        <a href="{{ path('app_register') }}" class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">S'inscrire</a>
                    </li>
                    <li>
                        <a href="{{ path('app_login') }}" class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">Se Connecter</a>
                    </li>
                {% endif %}

                <li>
                    <a href="{{ path('aboutUs') }}" class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">À propos de nous</a>
                </li>
                <li>
                    <a href="{{ path('contact') }}" class="block py-2 pl-3 pr-4 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 md:dark:hover:text-blue-500 dark:text-white dark:hover:bg-gray-700 dark:hover:text-white md:dark:hover:bg-transparent dark:border-gray-700">Contact</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<div class="espace"></div>


        {% block body %}
        {% endblock %}

    </div>

<footer class="footer bg-white rounded-lg shadow m-4 dark:bg-gray-800">
    <div class="w-full mx-auto max-w-screen-xl p-4 md:flex md:items-center md:justify-between">
      <span class=" text-sm textFooter">© 2023 <a href="{{ path('main_home') }}" class="texteRose hover:underline">Sortir.com™</a>. All Rights Reserved.
    </span>
        <ul class="flex flex-wrap items-center mt-3 text-sm font-medium text-gray-500 dark:text-gray-400 sm:mt-0">
            <li>
                <a href="{{ path('aboutUs') }}" class="texteNoir mr-4 hover:underline md:mr-6 ">À propos de nous</a>
            </li>
            <li>
                <a href="{{ path('privacy') }}" class="texteNoir mr-4 hover:underline md:mr-6">Privacy Policy</a>
            </li>
            <li>
                <a href="{{ path('contact') }}" class="texteNoir hover:underline">Contact</a>
            </li>
        </ul>
    </div>
</footer>


<script src="https://kit.fontawesome.com/a7dbd1cbd9.js" crossorigin="anonymous"></script>
    </body>
    {% block javascripts %}
    {% endblock %}
</html>
